{
  "name": "nvs",
  "version": "0.5.0",
  "description": "Cross-platform solution for using multiple versions of node. Useful for compatibility testing",
  "keywords": [
    "use",
    "node",
    "versions",
    "compatability",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/nvs.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "nvs": "./bin/nvs.js"
  },
  "scripts": {
    "compat": "./bin/nvs.js engines mocha-compat test/spec/**/*.test.js --no-timeouts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "node-version-use": "^0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cr": "^0.1.0",
    "cross-spawn-cb": "^0.3.1",
    "depcheck": "^1.0.0",
    "dis-dat": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "is-version": "^0.2.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5",
    "rimraf": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
