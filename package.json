{
  "name": "nvs",
  "version": "0.5.2",
  "description": "Cross-platform solution for using multiple versions of node. Useful for compatibility testing",
  "keywords": [
    "use",
    "node",
    "versions",
    "compatability",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/nvs.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "nvs": "./bin/nvs.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts",
    "test:engines": "nvu engines npm test"
  },
  "dependencies": {
    "node-version-use": "^0.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.7",
    "cr": "^0.1.0",
    "cross-spawn-cb": "^0.6.5",
    "depcheck": "^1.4.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "is-version": "^0.2.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=0.8"
  }
}
