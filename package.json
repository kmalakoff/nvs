{
  "name": "nvs",
  "version": "0.4.4",
  "description": "Run commands on multiple versions of node. Useful for compatibility testing.",
  "keywords": [
    "use",
    "node",
    "versions",
    "compatability",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/nvs.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "nvs": "./bin/nvs"
  },
  "scripts": {
    "compat": "node ./bin/nvs --engines -- mocha-compat test/spec/**/*.test.js  --no-timeouts",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.2.4",
    "end-with": "^1.0.2",
    "getopts-compat": "^2.2.5",
    "lodash.isarray": "^4.0.0",
    "lodash.uniq": "^4.5.0",
    "nave": "^3.2.1",
    "node-semvers": "^0.4.3",
    "node-version-use": "^0.1.6",
    "object-assign": "^4.1.1",
    "queue-cb": "^1.1.2",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=0.8"
  }
}
