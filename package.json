{
  "name": "nvs",
  "version": "0.3.0",
  "description": "Run commands on multiple versions of node. Useful for compatibility testing.",
  "keywords": [
    "use",
    "node",
    "versions",
    "compatability",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/nvs.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "nvs": "./bin/nvs"
  },
  "scripts": {
    "compat": "node ./bin/nvs --engines -- mocha-compat test/spec/**/*.test.js  --timeout=9999999",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "test": "mocha-compat test/spec/**/*.test.js --timeout=9999999"
  },
  "dependencies": {
    "commander": "^2.20.3",
    "cross-spawn-cb": "^0.2.2",
    "end-with": "^1.0.2",
    "lodash.isarray": "^4.0.0",
    "nave": "^3.2.1",
    "node-semvers": "^0.4.1",
    "node-version-use": "^0.1.1",
    "queue-cb": "^1.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=4"
  }
}
