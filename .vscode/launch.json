  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "mocha",
      "program": "${workspaceFolder}/node_modules/mocha-compat/bin/_mocha-compat",
      "args": ["--no-timeouts", "--colors", "${workspaceFolder}/test/spec/**/*.test.js"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cli",
      "program": "${workspaceFolder}/bin/nvs",
      "args": ["12", "node", "--version"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "compat",
      "runtimeExecutable": "C:\\Users\\Kevin\\.nvu\\installed\\v0.10.48\\node.exe",
      "protocol": "legacy",
      "program": "${workspaceFolder}/node_modules/mocha-compat/bin/_mocha-compat",
      "args": ["--no-timeouts", "--colors", "${workspaceFolder}/test/spec/**/*.test.js"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "memory",
      "cwd": "${workspaceFolder}/performance",
      "program": "${workspaceFolder}/performance/memory/index.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ops",
      "cwd": "${workspaceFolder}/performance",
      "program": "${workspaceFolder}/performance/operations/index.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
