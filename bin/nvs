#!/usr/bin/env node

var nvs = require('..');

if (process.argv.length < 4) {
  console.log('Missing arguments. Example usage: nvs 0.10,4,6,14 [command]');
  return process.exit(-1);
}

nvs(process.argv[2].split(','), process.argv.slice(3), function (err) {
  if (err) {
    console.log(err.message);
    return process.exit(err.exitCode || -1);
  }
  process.exit(0);
});
